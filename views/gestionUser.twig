{% extends 'layout.twig' %}

{% block body %}
<table>
    <tr>
        <th>Username</th>
        <th>Email</th>
        <th>Actions</th>
    </tr>
    {% for user in users %}
    <tr>
        <td>{{ user.username }}</td>
        <td>{{ user.email }}</td>
        <td>
            <a href="/users/delete/{{ user.id }}">Delete</a>
        </td>
    </tr>
    {% endfor %}
{% endblock %}
